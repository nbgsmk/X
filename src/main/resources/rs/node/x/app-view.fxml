      </children>
   </HBox>
   <HBox prefHeight="200.0" prefWidth="410.0" VBox.vgrow="ALWAYS">
      <children>
         <ListView fx:id="lv_customthing" maxWidth="1.7976931348623157E308" prefWidth="400.0" />
